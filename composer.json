{
    "name": "rapids/rapids",
    "description": "Rapid is a Laravel command can helps to generate crud, models, migration and ui with Livewire and Filament",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rapids\\Rapids\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rapids\\Rapids\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0",
        "filament/filament": "^3.2",
        "laravel/prompts": "^0.3.5"
    },
    "authors": [
        {
            "name": "Tresor kasenda",
            "email": "tresorkasendat@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Rapids\\Rapids\\RapidsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
