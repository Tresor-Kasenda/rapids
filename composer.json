{
    "name": "rapids/rapids",
    "description": "Package Laravel de génération de modèles et relations",
    "keywords": [
        "laravel",
        "models",
        "relations",
        "generator"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rapids\\Rapids\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "laravel/prompts": "^0.1.13"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "authors": [
        {
            "name": "Tresor Kasenda",
            "email": "tresorkasendat@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Rapids\\Rapids\\RapidsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
